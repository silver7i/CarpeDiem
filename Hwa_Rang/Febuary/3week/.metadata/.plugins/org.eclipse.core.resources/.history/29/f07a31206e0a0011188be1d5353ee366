import java.util.Arrays;
import java.util.Scanner;

public class BOJ_8979_올림픽 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		int k = sc.nextInt();
		
		int[][] olympic = new int[n][n];
		
		for(int i=0; i<n; i++) {
			for(int j=0; j<n; j++) {
				olympic[i][j] = sc.nextInt();				
			}
		}
		
		Arrays.sort(olympic);
		
		System.out.println(Arrays.toString(olympic));
		
		
		
	}
}














