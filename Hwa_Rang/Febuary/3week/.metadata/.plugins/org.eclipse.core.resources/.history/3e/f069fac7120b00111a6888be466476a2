import java.util.Arrays;
import java.util.Scanner;

public class BOJ_2605_줄세우기 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		
		int[] arr = new int[n];
		
		for(int i=1; i<=n; i++) {
			int idx = sc.nextInt();
			arr[(i-1)-idx] = i;
		}
		
		System.out.println(Arrays.toString(arr));
	}
}
